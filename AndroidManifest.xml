   <application android:allowBackup="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@style/AppTheme">
        
        <activity android:label="@string/app_name" android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        
         <activity android:name="com.moodstream.activity.StartActivity"/>
         
         
         <activity android:name=".activity.LoginActivity"/>
        
        <activity android:name="com.moodstream.activity.CreateUserActivity"/>
        
        <activity android:name="com.moodstream.activity.EventDetailsActivity"/>
        
        <activity android:name="com.moodstream.activity.TakePhotoActivity"/>
        
        <activity android:name="com.moodstream.activity.CreateEventActivity"/>
        
        <activity android:name="com.moodstream.activity.CreatePhotoActivity"/>
        
         <activity android:name="com.moodstream.activity.AddFriendActivity"/>
        
        <service android:name=".activity.GCMIntentService"/>
        <receiver android:name="com.google.android.gcm.GCMBroadcastReceiver" android:permission="com.google.android.c2dm.permission.SEND">
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE"/>
                <category android:name="com.moodstream"/>
            </intent-filter>
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.REGISTRATION"/>
                <category android:name="com.moodstream"/>
            </intent-filter>
        </receiver>
    
        <activity android:launchMode="singleTop" android:name=".activity.RegisterActivity"/>
</application>

</manifest>